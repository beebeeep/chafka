pub mod example;

pub struct Ingester;

#[derive(Deserialize)]
struct row {
    pub key: Uuid,
    pub value: i64,
}

impl super::Ingester for Ingester {
    fn get_name(self) -> String {
        "example-ingester"
    }
    fn decode(self, message: Vec<u8>) -> Result<Vec<Value>, anyhow::Error> {
        let r: row = serde_json::from_slice(message)?;
    }
}
